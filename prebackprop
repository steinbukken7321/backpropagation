import math

# Definição das entradas e pesos
i1 = 0.05
i2 = 0.10

w1 = 0.15
w2 = 0.20
w3 = 0.25
w4 = 0.30

w5 = 0.40
w6 = 0.45
w7 = 0.50
w8 = 0.55

b1 = 0.35
b2 = 0.60

# Cálculo das saídas
h1 = i1 * w1 + i2 * w3 + b1
h2 = i1 * w2 + i2 * w4 + b1

o1 = h1 * w5 + h2 * w7 + b2
o2 = h1 * w6 + h2 * w8 + b2

# Exibição das saídas
print("Saídas da rede neural:")
print("O1:", o1)
print("O2:", o2)
